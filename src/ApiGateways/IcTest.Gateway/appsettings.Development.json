{
  "ReverseProxy": {
    "Routes": {
      "crypto-route": {
        "ClusterId": "crypto-cluster",
        "RateLimiterPolicy": "cryptoApiLimit",
        "Match": {
          "Path": "/crypto-api/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "{**catch-all}" }
        ]
      },
      "importer-route": {
        "ClusterId": "importer-cluster",
        "RateLimiterPolicy": "cryptoApiLimit",
        "Match": {
          "Path": "/importer-api/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "{**catch-all}" }
        ]
      }
    },
    "Clusters": {
      "crypto-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://blocks.api:8080/"
          }
        }
      },
      "importer-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://blocks.importer:8080/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
